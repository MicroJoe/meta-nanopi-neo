			clock-output-names = "mmc1", "mmc1_output", "mmc1_sample";
			linux,phandle = <0x17>;
			phandle = <0x17>;
		};

		clk@01c20090 {
			#clock-cells = <0x1>;
			compatible = "allwinner,sun4i-a10-mmc-clk";
			reg = <0x1c20090 0x4>;
			clocks = <0x2 0x3 0x0 0xd>;
			clock-output-names = "mmc2", "mmc2_output", "mmc2_sample";
			linux,phandle = <0x18>;
			phandle = <0x18>;
		};

		clk@01c200cc {
			#clock-cells = <0x1>;
			#reset-cells = <0x1>;
			compatible = "allwinner,sun8i-h3-usb-clk";
			reg = <0x1c200cc 0x4>;
