			#clock-cells = <0x0>;
			clock-div = <0x1>;
			clock-mult = <0x1>;
			clocks = <0x2>;
			clock-output-names = "apb0";
			linux,phandle = <0xf>;
			phandle = <0xf>;
		};

		clk@01f01428 {
			compatible = "allwinner,sun8i-h3-apb0-gates-clk", "allwinner,sun4i-a10-gates-clk";
			reg = <0x1f01428 0x4>;
			#clock-cells = <0x1>;
			clocks = <0xf>;
			clock-indices = <0x0 0x1>;
			clock-output-names = "apb0_pio", "apb0_ir";
			linux,phandle = <0x23>;
			phandle = <0x23>;
		};

