			clock-div = <0x2>;
			clock-mult = <0x1>;
			clocks = <0x3 0x0>;
			clock-output-names = "pll6d2";
			linux,phandle = <0x9>;
			phandle = <0x9>;
		};

		pll8_clk {
			#clock-cells = <0x0>;
			compatible = "fixed-clock";
			clock-frequency = <0x1>;
			clock-output-names = "pll8";
